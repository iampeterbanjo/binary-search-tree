{
  "name": "binary-search-tree",
  "version": "1.0.0",
  "description": "Binary Search Tree in Typescript",
  "main": "build/index.js",
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "ava",
    "watch-test": "ava --watch",
    "prepare": "husky install",
    "lint": "prettier --check .",
    "lint-fix": "prettier --write ."
  },
  "devDependencies": {
    "@ava/typescript": "^2.0.0",
    "@sindresorhus/tsconfig": "^1.0.2",
    "ava": "^3.15.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "prettier": "2.6.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iampeterbanjo/binary-search-tree.git"
  },
  "keywords": [
    "binary-search-tree"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/iampeterbanjo/binary-search-tree/issues"
  },
  "homepage": "https://github.com/iampeterbanjo/binary-search-tree#readme"
}
